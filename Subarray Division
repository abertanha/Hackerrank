int birthday(int s_count, int* s, int d, int m)
{
    int Sucesso = 0;
    
        for (int i = 0; i < s_count; i++)
        {
            int Somador = 0;
            
            for (int j = i; j < (i + m); j++)
                Somador += s[j];
                
            if (Somador == d)
                Sucesso++;       
        }       
    return Sucesso;
}
