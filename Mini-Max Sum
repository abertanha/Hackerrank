function miniMaxSum(arr) {
    let min = arr[0];
    let max = arr[0];
    let sum = 0;
    for(let i = 0; i < arr.length; i++){
        sum += arr[i];
        if (max < arr[i]){
            max = arr[i];
        }
        if (min > arr[i]){
            min = arr[i];
        }                            
    }
    console.log((sum - max), (sum - min));
}
